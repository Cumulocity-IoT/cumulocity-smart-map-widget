    "@angular/router": "~11.1.2",
    "@c8y/client": "^1011.0.5",
    "@c8y/ngx-components": "1011.0.5",
    "@c8y/style": "1011.0.5",
    "angular-resize-event": "^2.1.0",
    "@ng-select/ng-select": "^6.1.0",
    "core-js": "^3.15.1",
    "fontawesome": "4.7.2",
    "font-awesome": "^4.7.0",
    "group-array": "^1.0.0",
    "hammerjs": "^2.0.8",
    "leaflet-extra-markers": "^1.2.1",
    "leaflet.markercluster": "^1.4.1",
    "leaflet2": "npm:leaflet@1.7.1",
    "leaflet-draw": "^1.0.4",
    "moment": "^2.26.0",
    "ngx-bootstrap": "6.2.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
