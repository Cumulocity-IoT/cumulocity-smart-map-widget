{
  "name": "gp-smart-map-project",
  "version": "2.1.0",
  "description": "The Smart Map widget help you to track real-time device locations in indoor with multi floor infrastructure as well as in outdoor. Smart Map also support real-time Heat map for Indoor and outdoor location events.",
  "author": "Darpankumar Lalani - Software AG, Global Presales ",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildPatch": "cd projects/gp-smart-map && npm version patch && ng build gp-smart-map && cd ../../  && cd ./dist/gp-smart-map && npm pack && move *.tgz ../",
    "buildMinor": "cd projects/gp-smart-map && npm version minor && ng build gp-smart-map && cd ../../ && cd ./dist/gp-smart-map && npm pack && move *.tgz ../",
    "buildMajor": "cd projects/gp-smart-map && npm version major && ng build gp-smart-map && cd ../../ && cd ./dist/gp-smart-map && npm pack && move *.tgz ../",
    "serve": "ng build gp-smart-map && npm i dist/gp-smart-map && ng s",
    "copyAssets": "ncp ./projects/gp-smart-map/src/lib/assets/ ./dist/gp-smart-map/assets",
    "runtime": "gulp --gulpfile ./runtime/gulpfile.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/common": "~11.1.2",
